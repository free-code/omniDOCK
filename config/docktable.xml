<?xml version="1.0"?>

<docktable-config>
        <rows>16</rows>
        <columns>4</columns>
	<launcher>
            <name>Firefox</name>
	    <icon>images/firefox.png</icon>
	    <exec>firefox</exec>
            <left_attach>0</left_attach>
            <right_attach>1</right_attach>
            <top_attach>0</top_attach>
            <bottom_attach>1</bottom_attach>
        </launcher>
        <launcher>
            <name>File Manager</name>
	    <icon>images/file-manager.png</icon>
	    <exec>nautilus</exec>
            <left_attach>1</left_attach>
            <right_attach>2</right_attach>
            <top_attach>0</top_attach>
            <bottom_attach>1</bottom_attach>
        </launcher>
        <launcher>
            <name>Terminal</name>
	    <icon>images/terminal.png</icon>
	    <exec>gnome-terminal</exec>
            <left_attach>2</left_attach>
            <right_attach>3</right_attach>
            <top_attach>0</top_attach>
            <bottom_attach>1</bottom_attach>
        </launcher>
        <notifier>
            <service>facebook</service>
            <left_attach>2</left_attach>
            <right_attach>3</right_attach>
            <top_attach>1</top_attach>
            <bottom_attach>2</bottom_attach>
        </notifier>
</docktable-config>